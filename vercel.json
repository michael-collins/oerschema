{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "headers": [
    {
      "source": "/(.*)\\.ttl",
      "headers": [
        { "key": "Content-Type", "value": "text/turtle" },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Vary", "value": "Accept" }
      ]
    },
    {
      "source": "/(.*)\\.jsonld",
      "headers": [
        { "key": "Content-Type", "value": "application/ld+json" },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Vary", "value": "Accept" }
      ]
    },
    {
      "source": "/(.*)\\.rdf",
      "headers": [
        { "key": "Content-Type", "value": "application/rdf+xml" },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Vary", "value": "Accept" }
      ]
    },
    {
      "source": "/(.*)\\.nt",
      "headers": [
        { "key": "Content-Type", "value": "application/n-triples" },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Vary", "value": "Accept" }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Vary", "value": "Accept" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/schema.ttl",
      "has": [
        {
          "type": "header",
          "key": "Accept",
          "value": "text/turtle"
        }
      ]
    },
    {
      "source": "/",
      "destination": "/schema.jsonld",
      "has": [
        {
          "type": "header",
          "key": "Accept",
          "value": "application/ld\\+json"
        }
      ]
    },
    {
      "source": "/",
      "destination": "/schema.rdf",
      "has": [
        {
          "type": "header",
          "key": "Accept",
          "value": "application/rdf\\+xml"
        }
      ]
    },
    {
      "source": "/",
      "destination": "/schema.nt",
      "has": [
        {
          "type": "header",
          "key": "Accept",
          "value": "application/n-triples"
        }
      ]
    },
    {
      "source": "/:term",
      "destination": "/terms/:term.ttl",
      "has": [
        {
          "type": "header",
          "key": "Accept",
          "value": "text/turtle"
        }
      ]
    },
    {
      "source": "/:term",
      "destination": "/terms/:term.jsonld",
      "has": [
        {
          "type": "header",
          "key": "Accept",
          "value": "application/ld\\+json"
        }
      ]
    },
    {
      "source": "/:term",
      "destination": "/terms/:term.rdf",
      "has": [
        {
          "type": "header",
          "key": "Accept",
          "value": "application/rdf\\+xml"
        }
      ]
    },
    {
      "source": "/:term",
      "destination": "/terms/:term.nt",
      "has": [
        {
          "type": "header",
          "key": "Accept",
          "value": "application/n-triples"
        }
      ]
    }
  ]
}